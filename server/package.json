{
  "name": "vuewebagk",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "serve": "nodemon src/server.js",
    "serve:ts": "nodemon -L --watch './**/*.ts' --exec ts-node src/server.ts",
    "gts": "gts fix",
    "fix": "npx eslint --fix",
    "ts": "nodemon src/server.ts",
    "typeorm:1": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm:1 migration:generate -- -n",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.5.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.15",
    "prettier-eslint": "^13.0.0",
    "prettier-stylelint": "^0.4.2",
    "prettier-tslint": "^0.4.2"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mariadb": "^2.5.6",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "ts-node": "^10.5.0",
    "typeorm": "^0.2.44",
    "typeorm-extension": "^1.1.0",
    "typescript": "^4.5.5"
  }
}
